<li id="application-<%= application.id %>">
    <h5><%= application.name %> - </h5>
    <ul>
        <li>Nyckel: <strong><%= application.key %></strong></li>
        <li>
            <span class="timestamp">
            Skapad <%= time_ago_in_words(application.created_at) %> ago.
            </span>
            <% if current_user_or_admin?(current_user) %>
            	<%= link_to "ta bort applikationen", application, method: :delete,
            	data: { confirm: "Är du säker att du vill ta bort denna app?" } %>
            <% end %>
        </li>
    </ul>
</li>